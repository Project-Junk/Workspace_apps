<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/memory_usage"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory>
        <com.color.support.preference.ColorPreference android:layout="@layout/preference_memory_app_head" android:key="app_icon_name" />
        <com.android.settings.SummaryPreference android:selectable="false" android:key="status_header" />
        <com.color.support.preference.ColorPreference android:layout="@layout/setting_memory_summary_preference" android:title="@string/running_frequency" android:key="frequency" />
        <com.color.support.preference.ColorPreference android:layout="@layout/setting_memory_summary_preference" android:title="@string/memory_maximum_usage" android:key="max_usage" />
